<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bsoft.mob.ienr.fragment.AdvisoryListFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <com.bsoft.mob.ienr.view.BsoftActionBar
            android:id="@+id/actionbar"
            style="@style/ClassicActionBar" />
        <com.wuxiaolong.pullloadmorerecyclerview.PullLoadMoreRecyclerView
            android:id="@+id/recycleview_advisory"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></com.wuxiaolong.pullloadmorerecyclerview.PullLoadMoreRecyclerView>
    </LinearLayout>
<TextView
    android:id="@+id/tv_not_data"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textSize="16sp"
    android:text="暂无数据"
    android:layout_gravity="center"/>
</FrameLayout>
