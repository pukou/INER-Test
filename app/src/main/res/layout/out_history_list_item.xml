<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/ClassicListItemVertical"
 >

    <TextView
        android:id="@+id/out_history_outtime_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:lineSpacingMultiplier="1"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/out_history_backtime_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:lineSpacingMultiplier="1"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/out_history_plan_backtime_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:lineSpacingMultiplier="1"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
            android:id="@+id/out_history_wcyy_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:lineSpacingMultiplier="1"
            android:text="131212"
            android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/out_history_with_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="10dp"
        android:lineSpacingMultiplier="1"
        android:textAppearance="?android:attr/textAppearanceSmall" />



</LinearLayout>