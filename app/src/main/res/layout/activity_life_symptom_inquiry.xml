<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/ClassicLinearV"
    tools:context=".activity.user.LifeSymptomInquiryActivity">

    <FrameLayout style="@style/ClassicFrame.MatchWrap">

        <com.bsoft.mob.ienr.view.BsoftActionBar
            android:id="@+id/actionbar"
            style="@style/ClassicActionBar" />

        <TextView
            android:id="@+id/textname"
            style="@style/ClassicTextSecondaryWhite"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="@dimen/classic_item_comm_margin_primary"
            android:drawableRight="@drawable/selector_classic_icon_up_down"
            tools:text="31231231" />

    </FrameLayout>

    <include layout="@layout/layout_bar_date_double_icon" />


    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/id_swipe_refresh_layout"
        style="@style/ClassicSwipe.MatchAuto">

        <FrameLayout style="@style/ClassicFrame">

            <ListView
                android:id="@+id/id_lv"
                style="@style/ClassicListView"
                tools:listitem="@layout/item_list_text_five_vert_primary" />
        </FrameLayout>
    </android.support.v4.widget.SwipeRefreshLayout>


</LinearLayout>